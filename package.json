  "devDependencies": {
    "@types/debug": "^4.1.4",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.6.2",
    "@types/prettier": "^1.16.4",
    "@types/rimraf": "^2.0.2",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "class-validator-jsonschema": "^1.3.0",
    "codecov": "^3.5.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tsutils": "^3.14.0",
    "typescript": "*"
